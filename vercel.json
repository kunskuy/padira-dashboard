{
  "routes": [
    {
      "src": "/data/(.*)",
      "dest": "/public/data/$1",
      "headers": {
        "Content-Type": "application/json"
      }
    },
    {
      "src": "/maps/(.*)",
      "dest": "/public/maps/$1",
      "headers": {
        "Content-Type": "application/json"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}