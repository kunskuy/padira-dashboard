{
  "functions": {
    "app.js": {
      "includeFiles": "public/**"
    }
  },
  "rewrites": [
    {
      "source": "/data/(.*)",
      "destination": "/data/$1"
    },
    {
      "source": "/maps/(.*)",
      "destination": "/maps/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/data/(.*)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/json"
        }
      ]
    },
    {
      "source": "/maps/(.*)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/json"
        }
      ]
    }
  ]
}